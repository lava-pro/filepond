{
    "name": "packages/filepond",
    "description": "Filepond Local Package",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^5.8.10"
    },
    "autoload": {
        "psr-4": {
            "Lava\\Filepond\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lava\\Filepond\\FilepondServiceProvider"
            ],
            "aliases": {
                "Filepond": "Lava\\Filepond\\Facades\\Filepond"
            }
        }
    }
}
